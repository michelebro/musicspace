<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head") %>
  <style>
    div {
        display: grid;
    }
  </style>

<body>
    <nav>
        <a href="/musicspace/new">add a new song</a>
    </nav>
    <h1>my queue</h1>
        <div>
         <% songs.forEach(songs => { %>
                <a href="/musicspace/<%= songs._id %>">
                    <%= songs.songTitle %> 
                </a>
                <form action="/musicspace/<%= songs._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="x" class="deleteBtn">
                </form>
                <a href="/musicspace/<%= songs._id %>/edit"><button>edit</button></a><br><br>
        <% }); %>
    </div>
</body>
</html>